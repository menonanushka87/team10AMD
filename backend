# Ectracting CSV
import pandas as pd

# Replace file_path.csv' with the path to your CSV file
file_path = 'file_path.csv'

# Read the CSV file into a pandas DataFrame
data = pd.read_csv(file_path)

# Display the first few rows of the DataFrame
print(data.head())


# Extracting SARIF
pip install sarif
from sarif import parse

# Replace 'file_path.sarif' with the path to your SARIF file
file_path = 'file_path.sarif'

# Open the SARIF file and parse its contents
with open(file_path, 'r', encoding='utf-8') as sarif_file:
    sarif_data = parse(sarif_file)

# Extracting information from the parsed SARIF data
results = sarif_data.runs[0].results if sarif_data.runs else []

# Printing the results
for result in results:
    print("Rule ID:", result.rule_id)
    print("Message:", result.message.text)
    print("Severity:", result.level)
    print("Locations:")
    for location in result.locations:
        print("  File:", location.physical_location.artifact_location.uri)
        print("  Line:", location.physical_location.region.start_line)
    print()

